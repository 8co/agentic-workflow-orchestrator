{
  "name": "agentic-workflow-orchestrator",
  "version": "0.1.0",
  "description": "An autonomous agent orchestration system that enables continuous software development through scheduled AI workflows. Integrates with Cursor, Copilot, and Codex APIs to execute development tasks via programmatic prompts in an AWS serverless architecture.",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "cli": "tsx src/cli.ts",
    "workflow:run": "tsx src/cli.ts run",
    "workflow:list": "tsx src/cli.ts list",
    "docs:list": "./scripts/docs-list.sh",
    "docs:cat": "./scripts/docs-list.sh --cat",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "ai-agents",
    "llm-orchestration",
    "autonomous-development",
    "aws-lambda",
    "typescript",
    "serverless",
    "workflow-automation",
    "cursor-api",
    "github-copilot"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^25.2.3",
    "@types/uuid": "^10.0.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "uuid": "^13.0.0",
    "yaml": "^2.8.2"
  }
}
