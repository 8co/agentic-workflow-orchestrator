{
  "name": "agentic-workflow-orchestrator",
  "version": "0.1.0",
  "description": "Autonomous AI agent system for continuous full-stack development. LLM-driven code generation, verification, and self-correction in a closed loop.",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "cli": "tsx src/cli.ts",
    "workflow:run": "tsx src/cli.ts run",
    "workflow:list": "tsx src/cli.ts list",
    "docs:list": "./scripts/docs-list.sh",
    "docs:cat": "./scripts/docs-list.sh --cat",
    "test": "node --test",
    "verify": "tsc --noEmit",
    "dashboard": "tsx src/dashboard-server.ts"
  },
  "keywords": [
    "ai-agents",
    "llm-orchestration",
    "autonomous-development",
    "aws-lambda",
    "typescript",
    "serverless",
    "workflow-automation",
    "cursor-api",
    "github-copilot"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^25.2.3",
    "@types/uuid": "^10.0.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.74.0",
    "dotenv": "^17.3.1",
    "openai": "^6.21.0",
    "uuid": "^13.0.0",
    "yaml": "^2.8.2"
  }
}
